class Bookshelf:
    def __init__(self,name,author,price,release):
        self.bookName = name
        self.bookAuthor = author
        self.bookPrice = price
        self.bookRelease = release
        
    def addBook(self):
        print("Name:" + self.bookName)
        print("Book Author:" + self.bookAuthor)
        print("Price:" + self.bookPrice)
        print("Release Date" + self.bookRelease)
        print("Book Added")
        
book1 = Bookshelf("Diary of a Wimpy Kid","Jeff Kinney","5","4/1/2007")
Bookshelf.addBook()

book2 = Bookshelf("Diary of a Wimpy Kid: Rodrick Rules ","Jeff Kinney","5","3/25/2011")
Bookshelf.addbook()
